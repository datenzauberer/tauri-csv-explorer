{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-csv-explorer",
  "version": "0.0.1",
  "identifier": "de.datenzauberer.tauri-csv-explorer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri-csv-explorer",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "fileAssociations": [
      {
        "ext": [
          "csv"
        ],
        "name": "CSV File",
        "role": "Editor",
        "mimeType": "text/csv",
        "description": "A comma-separated values file."
      }
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {

  "updater": {
    "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgzMkZCRUYyRkIwNjlDNjIKUldSaW5BYjc4cjR2ZzdaM2hQRkdoYWhVMHArRHZjVFRxTnB6ZEE5em1kWmtaRkdOc0ZCVk5lZGIK",
    "endpoints": [
        "https://github.com/datenzauberer/tauri-csv-explorer/releases/latest/download/latest.json"
    ]
  }
  }
}
